{
  "name": "syntax/steam-api",
  "description": "A steam-api client for Laravel 5/6/7.",
  "license": "MIT",
  "authors": [
    {
      "name": "Stygian",
      "email": "stygian.warlock.v2@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "laravel/framework": "^5.0|^6.0|^7.0",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~3.0",
    "mockery/mockery": "0.9.3",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Syntax\\SteamApi\\": "src/Syntax/SteamApi"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
        "providers": [
            "Syntax\\SteamApi\\SteamApiServiceProvider"
        ],
        "aliases": {
            "Steam": "Syntax\\SteamApi\\Facades\\SteamApi"
        }
    }    
  },
  "minimum-stability": "stable"
}
